<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="RegisterControllers" type="MVCEngine.AppConfig.ControllerSection, MVCEngineLibrary" />
    <section name="RegisterViews" type="MVCEngine.AppConfig.ViewSection, MVCEngineLibrary" />
  </configSections>
  <RegisterControllers>
    <Controllers>
      <Controller name="Worksheet" class="CtgWorksheet.Controllers.WorksheetController" assembly="mfc-for-net"/>
      <Controller name="Screening" class="CtgWorksheet.Controllers.ScreeningController" assembly="mfc-for-net"/>
    </Controllers>
  </RegisterControllers>
  <RegisterViews>
    <Views>
      <View name="Worksheet" class="MvcForNet.CtgWorksheet.GUI.WorksheetForm" assembly="mfc-for-net"/>
      <View name="Screening" class="MvcForNet.CtgWorksheet.GUI.ScreeningControl" assembly="mfc-for-net"/>
    </Views>
  </RegisterViews>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
